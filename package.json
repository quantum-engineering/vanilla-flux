{
  "name": "vanilla-flux",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify public/javascripts/src/main.js -d -o public/javascripts/bundle.js",
    "watch": "watchify public/javascripts/src/main.js -d -o public/javascripts/bundle.js -v & cssnext --watch public/stylesheets/main.css public/stylesheets/bundle.output.css --verbose",
    "build-prod": "browserify public/javascripts/src/main.js -o public/javascripts/bundle.js && uglifyjs public/javascripts/bundle.js -o public/javascripts/bundle.js -v"
  },
  "browserify": {
    "transform": [
      "babelify",
      "flow-typestrip",
      "browserify-shim"
    ]
  },
  "browser": {
    "react": "./node_modules/react/dist/react-with-addons.min.js",
    "jquery": "./node_modules/jquery/dist/jquery.min.js"
  },
  "browserify-shim": {
    "react": "React",
    "jquery": "$"
  },
  "dependencies": {
    "body-parser": "1.8.1",
    "cookie-parser": "1.3.3",
    "cssnano": "^2.1.1",
    "debug": "2.0.0",
    "express": "4.9.0",
    "faker": "^3.0.0",
    "flux": "^2.0.3",
    "jquery": "2.1.4",
    "morgan": "1.3.0",
    "node-uuid": "^1.4.3",
    "object-assign": "^3.0.0",
    "react": "0.13.3",
    "serve-favicon": "2.1.3",
    "superagent": "^1.2.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babelify": "6.1.3",
    "browserify": "11.0.0",
    "browserify-shim": "3.8.10",
    "cssnext": "^1.8.3",
    "flow-typestrip": "0.1.3",
    "watchify": "3.3.0"
  }
}
